generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/@prisma/client/estudiantes"
}

datasource db {
  provider = "postgresql"
}

model Estudiante {
  id        Int      @id @default(autoincrement())
  nombre    String
  email     String   @unique
  estado    String   @default("ACTIVO")
  carreraId Int?    
}